		"eslint-config-neon": "^0.1.57",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^15.1.0",
		"prettier": "^3.1.0",
		"prettier-eslint": "^16.1.2",
		"typescript": "^5.3.2",
		"vitest": "^0.34.6"
	},
	"dependencies": {
		"@sapphire/stopwatch": "^1.5.0",
		"tslib": "^2.6.2"
	}
